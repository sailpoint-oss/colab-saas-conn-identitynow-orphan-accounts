{
    "name": "orphan-accounts",
    "version": "0.4.0",
    "main": "dist/index.js",
    "scripts": {
        "clean": "shx rm -rf ./dist",
        "prebuild": "npm run clean",
        "build": "npx ncc build ./src/index.ts -o ./dist -m -C",
        "dev": "cross-env NODE_OPTIONS=--enable-source-maps spcx run dist/index.js",
        "debug": "spcx run dist/index.js",
        "prettier": "npx prettier --write .",
        "test": "jest --coverage --passWithNoTests",
        "prepack-zip": "npm ci && npm run build",
        "pack-zip": "spcx package"
    },
    "private": true,
    "dependencies": {
        "@sailpoint/connector-sdk": "1.1.24",
        "sailpoint-api-client": "^1.6.1",
        "axios-retry": "^4.0.0",
        "form-data": "^4.0.0",
        "orphan-accounts": "file:"
    },
    "devDependencies": {
        "@types/node": "^22.1.0",
        "@vercel/ncc": "^0.38.1",
        "prettier": "^3.2.5",
        "shx": "^0.3.4",
        "typescript": "^5.3.3",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "@types/jest": "^30.0.0"
    },
    "prettier": {
        "printWidth": 120,
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    }
}
